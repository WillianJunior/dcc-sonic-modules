#%Module1.0#####################################################################
##
## python3.10.12 modulefile
##
proc ModulesHelp { } {
        puts stderr "\tAdds Python 3.10.12 to your PATH environment variable.\nIt should be called through 'python' instead of 'python3'."
}

module-whatis   "adds Python 3.12.1"

prepend-path    PATH    /opt/python/3-10-12/bin
conflict        python
set-alias       python python3.10
